  DigiKeyboard.delay(1000);
  DigiKeyboard.sendKeyStroke(0);
}

void loop() {
  DigiKeyboard.SendKeyStroke(KEY_D, MOD_GUİ_LEFT); 
  DigiKeyboard.delay(1000);
